            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:text="TextView"
            android:layout_weight="6.5"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#000000" />
    </LinearLayout>

